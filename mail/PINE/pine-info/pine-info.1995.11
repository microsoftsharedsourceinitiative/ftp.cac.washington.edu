  # Use these for the gcc compiler
  # CC=	gcc
--- 69,81 ----
  LOCLIBES=    $(PICODIR)/libpico.a $(IMAPDIR)/c-client.a
  
  # Use these for the Solaris C compiler
! CFLAGS= -DSV4 $(OPTIMIZE) $(PROFILE) $(DEBUG) -DANSI -DSYSTYPE=\"SOL\"
