***************
*** 69,81 ****
  LOCLIBES=    $(PICODIR)/libpico.a $(IMAPDIR)/c-client.a
  
  # Use these for the Solaris C compiler
! CFLAGS= -DSV4 $(OPTIMIZE) $(PROFILE) $(DEBUG) -DSYSTYPE=\"SOL\"
