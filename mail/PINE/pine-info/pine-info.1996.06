  #include "mail.h"
+ #define _USEC_UTIME_FLAVOR
+ #include <utime.h>
  #include "osdep.h"
  #include <stdio.h>
***************
*** 48,56 ****
  extern int errno;		/* just in case */
  #include <pwd.h>
- #include <utime.h>
